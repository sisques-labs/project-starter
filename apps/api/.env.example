# Environment
NODE_ENV=development
PORT=4100
API_PREFIX=api      

# PostgreSQL Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/master"
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USERNAME=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DATABASE=master

# MongoDB Database
MONGODB_URI="mongodb://mongo:mongo@localhost:27017/master"
MONGODB_HOST=localhost
MONGODB_PORT=27017
MONGODB_USERNAME=mongo
MONGODB_PASSWORD=mongo
MONGODB_DATABASE=master
MONGODB_MAX_POOL_SIZE=10
MONGODB_MIN_POOL_SIZE=2
MONGODB_MAX_IDLE_TIME_MS=30000
MONGODB_WAIT_QUEUE_TIMEOUT_MS=0

# JWT
JWT_ACCESS_SECRET=your-super-secret-jwt-key-change-in-production
JWT_REFRESH_SECRET=secret
JWT_ACCESS_EXPIRATION=15m
JWT_REFRESH_EXPIRATION=7d


# CORS
CORS_ORIGIN=http://localhost:3000

# Logger
LOG_LEVEL=info

# Storage Provider Selection
# Options: S3, SUPABASE, SERVER_ROUTE
# Default: S3
STORAGE_PROVIDER=SUPABASE

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_S3_BUCKET_NAME=your_bucket_name
AWS_S3_REGION=us-east-1

# Supabase Configuration
SUPABASE_URL=https://supabaseurl.supabase.co
SUPABASE_KEY=supabase-api-key
SUPABASE_STORAGE_BUCKET=test

# Server Route Storage Configuration
STORAGE_SERVER_ROUTE_URL=https://your-storage-server.com
STORAGE_SERVER_ROUTE_API_KEY=your_api_key